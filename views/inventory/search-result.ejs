<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>

<% if (title) { %>
    <h1><%= title %></h1>
  <% } else { %>
    <%- message() %>
  <% } %>
  <% if (typeof grid !== 'undefined' && grid) { %>
    <%- grid %>
  <% } %>

